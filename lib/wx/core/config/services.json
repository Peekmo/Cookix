{
    "parameters": {
        "env": "%general.env%"
    },

    "services": [
        {
            "id": "configuration",
            "class": "wx.core.config.Configuration"
        },
        {
            "id": "routing",
            "class": "wx.core.routing.Routing",
            "parameters": [
                "@context@"
            ]
        },
        {
            "id": "resolver",
            "class": "wx.core.controller.Resolver"
        },
        {
            "id": "context",
            "class": "wx.core.context.Context",
            "parameters": [
                "%env%"
            ]
        }
    ]
}