{
    "comment": ["Parameters can only be used in routes"],
    "parameters": {
        "param": "%general.env%"
    },

    "routes": {
        "/test/%param%/:test/:id" : {
            "controller": "wx.demo.controllers.DemoController",
            "action": "test",
            "requirements": {
                "_methods": ["GET"],
                "test": "^[a-z]*$",
                "id": "^[0-9]+$"
            }
        }
    }
}